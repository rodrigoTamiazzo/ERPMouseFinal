<div class="row-fluid ">
  <div class="span12">
    <h6>Basic colors</h6>
    <div class="progress progress-light-blue">
      <div class="bar" style="width: 20%"></div>
    </div>
    <div class="progress progress-medium-blue">
      <div class="bar" style="width: 30%"></div>
    </div>
    <div class="progress progress-dark-blue">
      <div class="bar" style="width: 40%"></div>
    </div>
    <div class="progress progress-text-blue">
      <div class="bar" style="width: 50%"></div>
    </div>
    <div class="progress progress-dark-plum">
      <div class="bar" style="width: 60%"></div>
    </div>
  </div>
</div>

<div class="row-fluid ">
  <div class="span12">
    <h6>Extra colors</h6>
    <div class="progress progress-yellow">
      <div class="bar" style="width: 20%"></div>
    </div>
    <div class="progress progress-orange">
      <div class="bar" style="width: 30%"></div>
    </div>
    <div class="progress progress-red">
      <div class="bar" style="width: 40%"></div>
    </div>
    <div class="progress progress-plum">
      <div class="bar" style="width: 50%"></div>
    </div>
    <div class="progress progress-blue">
      <div class="bar" style="width: 60%"></div>
    </div>
    <div class="progress progress-turquoise">
      <div class="bar" style="width: 70%"></div>
    </div>
    <div class="progress progress-green">
      <div class="bar" style="width: 80%"></div>
    </div>
  </div>
</div>

<div class="row-fluid ">
  <div class="span12 padded5">
    <h6>Bootstrap</h6>
    <div class="progress progress-info">
      <div class="bar" style="width: 30%"></div>
    </div>
    <div class="progress progress-warning">
      <div class="bar" style="width: 50%"></div>
    </div>
    <div class="progress progress-success progress-striped">
      <div class="bar" style="width: 70%"></div>
    </div>
    <div class="progress progress-danger progress-striped active">
      <div class="bar" style="width: 80%"></div>
    </div>
  </div>
</div>
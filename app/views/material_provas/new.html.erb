<h2><%= @page_title = "Nova Requisicao" %></h2><hr />

<% if notice %>
  <div class="alert red fade in">
    <button type="button" class="close" data-dismiss="alert">&times;</button>
    <%= notice %>
  </div>
<% end %>

<% @material_prova = MaterialProva.new %>


<%= form_for @material_prova, :validate => true, :html => {:class => "form-horizontal"} do |f| %>
  
  <%= render(:partial => 'form', :locals => {:f => f}) %>    
  <div class="control-group">
    <div class="controls">
      <%= f.submit("Salvar", :class => "btn btn-small dark-blue") %>
    </div>
  </div>
<% end %>

<%= link_to 'Voltar para a lista de materiais', material_provas_path, :class => "btn btn-small light-blue inverse" %>
